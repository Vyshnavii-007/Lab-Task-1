#include<stdio.h>

int GCD(int a, int b);

int main(){
    int a,b,result;

    printf("Enter two numbers a and b : ");
    scanf("%d %d",&a,&b);

    if(a<0 || b<0){
        printf("~Enter positive numbers only.\n~");
    }

    result = GCD(a,b);

    printf("The GCD of %d and %d is : %d\n",a,b,result);

    return 0;
}
int GCD(int a , int b){
    if(b==0){
        return a;
    }
    return GCD(b,a%b);
}
